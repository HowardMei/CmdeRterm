function microcom {
  [ "$1" == "-s" ] && COMPORT=" on \033[36m$3\033[0m" || COMPORT="\033[0m"
  echo -e "\033[31mLaunching microcom serial connection$COMPORT.\n\033[31mPress Ctrl+X to exit.\033[0m"
  $CMDER_ROOT/bin/busybox.exe microcom "$@"
}

