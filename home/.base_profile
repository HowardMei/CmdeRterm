function extract() {
  if [ -z "$1" ]; then
    echo "Usage: extract <archive_to_extract>"
  elif [ -f $1 ]; then
    case $1 in
      *.tar.bz2)   tar -xvjf $1;;
      *.tar.gz)    tar -xvzf $1;;
      *.tar.xz)    tar -xvJf $1;;
      *.bz2)       bunzip2 $1;;
      *.gz)        gunzip $1;;
      *.xz)        unxz $1;;
      *.lzma)      unlzma $1;;
      *.tar)       tar -xvf $1;;
      *.tbz2)      tar -xvjf $1;;
      *.tgz)       tar -xvzf $1;;
      *.txz)       tar -xvJf $1;;
      *.zip)       unzip $1;;
      *.Z)         uncompress $1;;
      *)           echo "'$1' cannot be extracted via >extract<";;
    esac
  else
    echo "'$1' is not a valid file"
  fi
}
